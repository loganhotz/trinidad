\usepackage{xcolor}

% trinidad colors
\definecolor{tgreen}{RGB}{57, 115, 82}
\definecolor{tlightgreen}{RGB}{121, 217, 163}
\definecolor{tmarigold}{RGB}{242, 159, 5}
\definecolor{torange}{RGB}{217, 103, 4}
\definecolor{tred}{RGB}{115, 32, 2}
\definecolor{tcoral}{RGB}{242, 121, 131}
\definecolor{tblue}{RGB}{12, 99, 102}
\definecolor{tlightblue}{RGB}{20, 161, 166}
\definecolor{tlava}{RGB}{73, 65, 55}
\definecolor{tcoffee}{RGB}{58, 48, 45}



\mode<presentation>

% these control a lot of parts. to what extent is unknown (to me)
\setbeamercolor*{palette primary}{bg=, fg=tblue}
\setbeamercolor*{palette secondary}{bg=, fg=tlightblue}
\setbeamercolor*{palette tertiary}{bg=, fg=tblue!80}
\setbeamercolor*{palette quaternary}{bg=, fg=tlava}

% overall color is the trinidad blue
\setbeamercolor*{structure}{bg=, fg=tblue}

% title slide information
\setbeamercolor*{title}{fg=tblue}
\setbeamercolor*{title page header}{fg=tlava}
\setbeamercolor*{author}{fg=tlava}
\setbeamercolor*{date}{fg=tlava}

% text colors
\setbeamercolor*{normal text}{fg=tlava}
\setbeamercolor*{alerted text}{fg=tmarigold}

\mode
<all>
